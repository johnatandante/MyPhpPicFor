<?xml version="1.0" encoding="UTF-8" ?>
<parametri>
   <!-- il nome del parametro è definito dalla proprietà nome -->
   <parametro nome="uno">
      <!-- i tag specificano i vincoli sui valori del parametro (i nomi dei tag sono autoesplicativi) -->
      <maggiore>10</maggiore>
      <minore>20</minore>
   </parametro>
   <parametro nome="due">
      <!-- non tutti i vincoli sono obbligatori -->
      <maggiore>3.71</maggiore>
   </parametro>
   <!-- i vincoli possono anche non essere definiti affatto -->
   <parametro nome="terzo" />
   <!-- il seguente tag descrive il calcolo che dovrà essere eseguito.
   I possibili operandi sono: + - * / e la precedenza degli operatori è definita dalle solite regole matematiche.
   Le parentesi non sono previste -->
   <risultato>
      uno * due + tre / terzo + uno
   </risultato>
   
</parametri>
